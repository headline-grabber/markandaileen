out_name <- "Insurance Headlines"
output_folder <- "./3. Headline Reports"
r_markdown_link <- "./1. Script/3. Headline Report.Rmd"

rmarkdown::render(r_markdown_link,
                  output_dir = output_folder,
                  output_file = paste0(out_name,"_",to_date))

pagedown::chrome_print(
  paste0(output_folder,"/",out_name,"_",to_date,".html"), 
  paste0(output_folder,"/",out_name,"_",to_date,".pdf"),
  timeout = 20)
